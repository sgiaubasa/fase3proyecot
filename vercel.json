{
  "version": 2,
  "builds": [
    { "src": "api/[...all].js", "use": "@vercel/node" },
    { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/[...all].js" },
    { "src": "^(?!/api/).*", "dest": "frontend/dist/index.html" }
  ]
}